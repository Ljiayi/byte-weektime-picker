  "author": "vincentzyc",
  "keywords": [
    "vue",
    "component",
    "time-picker",
    "weektime",
    "bytetime",
    "weektime-picker",
    "bytetime-picker",
    "byte-weektime-picker"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentzyc/byte-weektime-picker"
  },
  "scripts": {
